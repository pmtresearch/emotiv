{
  "atom": {
    "rewriteTsconfig": false
  },
  "awesomeTypescriptLoaderOptions": {
    "useWebpackText": true,
    "forkChecker": true,
    "useBabel": false,
    "useCache": false,
    "cacheDirectory": "node_modules/.cache/ecma"
  },
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "../../../../",
    "declaration": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "jsx": "preserve",
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noEmitHelpers": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "pretty": true,
    "removeComments": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "strictFunctionTypes": true,
    "strictNullChecks": false,
    "stripInternal": false,
    "target": "esnext"
  },
  "exclude": [
    "node_modules",
    "../../node_modules/**/*",
    "../../lib/**/*",
    "../../bin/**/*",
    "../../public/**/*",
    "../../coverage/**/*",
    "../../builds/**/*",
    "../../static/**/*",
    "../../reports/**/*",
    "../../../../node_modules/**/*",
    "../../../../lib/**/*",
    "../../../../bin/**/*",
    "../../../../public/**/*",
    "../../../../coverage/**/*",
    "../../../../builds/**/*",
    "../../../../static/**/*",
    "../../../../reports/**/*"
  ],
  "include": [
    "../../**/*",
    "../../../../**/*"
  ]
}
