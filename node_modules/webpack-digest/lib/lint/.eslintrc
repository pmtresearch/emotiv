{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true
  },
  "globals": {
    "define": true,
    "angular": true
  },
  "extends": [
    "eslint:recommended",
    "canonical",
    "canonical/react",
    "plugin:jsx-a11y/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "babel",
    "angular",
    "react",
    "jsx-a11y"
  ],
  "rules": {
    "import/no-unresolved": "off",
    "import/no-unassigned-import": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
